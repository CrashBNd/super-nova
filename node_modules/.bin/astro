#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/xampp/htdocs/super-nova/node_modules/.pnpm/astro@5.13.2_jiti@2.5.1_lig_7470587b260869ea8952b3423f310733/node_modules/astro/node_modules:/mnt/c/xampp/htdocs/super-nova/node_modules/.pnpm/astro@5.13.2_jiti@2.5.1_lig_7470587b260869ea8952b3423f310733/node_modules:/mnt/c/xampp/htdocs/super-nova/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/xampp/htdocs/super-nova/node_modules/.pnpm/astro@5.13.2_jiti@2.5.1_lig_7470587b260869ea8952b3423f310733/node_modules/astro/node_modules:/mnt/c/xampp/htdocs/super-nova/node_modules/.pnpm/astro@5.13.2_jiti@2.5.1_lig_7470587b260869ea8952b3423f310733/node_modules:/mnt/c/xampp/htdocs/super-nova/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
